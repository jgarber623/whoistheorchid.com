{
  "name": "whoistheorchid.com",
  "private": true,
  "scripts": {
    "build": "bundle exec jekyll build --config jekyll.yml --profile --trace",
    "start": "bundle exec jekyll serve --config jekyll.yml --trace",
    "test": "$(npm bin)/npm-run-all --print-label test:*",
    "test:html": "bundle exec htmlproofer ./public --assume-extension --only-4xx --url-ignore /vimeo.com/",
    "test:javascript": "$(npm bin)/eslint src/_assets/javascripts",
    "test:sass": "$(npm bin)/sass-lint --no-exit --verbose"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "npm-run-all": "^4.1.5",
    "sass-lint": "^1.12.1"
  }
}
