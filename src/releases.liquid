---

<section class="releases">
  <h1 class="page-title">{{ title }}</h1>

  <ol class="releases-list ui-list clearfix" reversed>
    {% for release in collections.release reversed -%}
      {% include release with release %}
    {% endfor -%}
  </ol>
</section>

<section class="releases">
  <h1 class="page-title">Compilations</h1>

  <ol class="releases-list ui-list clearfix" reversed>
    {% for release in collections.compilation reversed -%}
      {% include release with release %}
    {% endfor -%}
  </ol>
</section>
